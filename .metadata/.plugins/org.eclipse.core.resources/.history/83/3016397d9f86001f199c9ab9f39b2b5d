package co.edu.uco.UcoBet.generales.application.secondaryports.entity;

import java.util.UUID;

import co.edu.uco.UcoBet.generales.domain.Domain;
import co.edu.uco.UcoBet.generales.domain.state.StateDomain;

public class CityEntity extends Domain{
	
	private String name;
	private StateDomain state;
	
	private CityEntity(UUID id,String name,StateDomain state) {
		super(id);
		setState(state);
		setName(name);
	}
	
	public static final CityEntity create(UUID id,String name,StateDomain state) {
		return new CityEntity(id, name, state);
	}
	
	
	public String getName() {
		return name;
	}
	private void setName(String name) {
		this.name = name;
	}
	public StateDomain getState() {
		return state;
	}
	private void setState(StateDomain state) {
		this.state = state;
	}
	
	

}
